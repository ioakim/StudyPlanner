<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="createProfileWindow" prefHeight="182.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="studyplanner.CreateStudyProfileViewController">
   <children>
      <Button fx:id="createProfileButton" layoutX="139.0" layoutY="147.0" mnemonicParsing="false" onAction="#createProfileButtonClick" text="Create Profile" />
      <Button fx:id="cancelProfileButton" layoutX="33.0" layoutY="147.0" mnemonicParsing="false" onAction="#cancelButtonClick" text="Cancel" />
      <Label layoutX="39.0" layoutY="36.0" text="Name:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="20.0" layoutY="79.0" text="Data File:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="profileNameField" layoutX="85.0" layoutY="36.0" prefHeight="25.0" prefWidth="132.0" promptText="Profile Name" />
      <TextField fx:id="dataFilePathField" editable="false" layoutX="85.0" layoutY="79.0" onMouseClicked="#browseButtonClick" prefHeight="25.0" prefWidth="132.0" promptText="Module information" />
      <Button fx:id="browseButton" layoutX="217.0" layoutY="79.0" mnemonicParsing="false" onAction="#browseButtonClick" prefHeight="25.0" prefWidth="30.0" text="..." />
      <Label fx:id="errNameLabel" ellipsisString="" layoutX="90.0" layoutY="19.0" prefHeight="17.0" prefWidth="123.0" textFill="RED" textOverrun="CLIP" />
      <Label fx:id="errDataLabel" ellipsisString="" layoutX="90.0" layoutY="61.0" prefHeight="17.0" prefWidth="123.0" textFill="RED" textOverrun="CLIP" />
   </children>
</AnchorPane>
